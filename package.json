{
  "name": "clean-architecture-node",
  "version": "1.0.0",
  "description": "Estudo sobre clean architecture e outras boas praticas com NodeJS",
  "main": "index.js",
  "scripts": {
    "depcheck": "depcheck --ignores=husky,lint-staged",
    "check": "standard",
    "coverage": "nyc report",
    "mocha": "mocha",
    "pretest": "npm run check && npm run depcheck && npm audit --production --audit-level=moderate",
    "test": "mocha",
    "test:ci": "nyc --reporter=lcov mocha && npm run coverage"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "engines": {
    "npm": "7.11.1",
    "node": "6.14.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rntvicente/clean-architecture-node.git"
  },
  "keywords": [
    "NodeJS",
    "Express",
    "MongoDB",
    "Design Partterns",
    "Injection Dependence",
    "TDD",
    "Mocha",
    "Chai"
  ],
  "author": "Renato Vicente",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rntvicente/clean-architecture-node/issues"
  },
  "homepage": "https://github.com/rntvicente/clean-architecture-node#readme",
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "depcheck": "^1.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "sinon": "^10.0.0",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "validator": "^13.6.0"
  }
}
